{% extends "GitonomyTicketingBundle::layout_tracker.html.twig" %}

{% trans_default_domain 'ticket_tracker' %}

{% block tracker_content %}
    <div id="content">
        <form method="POST" action"{{ path('project_create') }}" class="form-horizontal" novalidate>
        <legend>{{ 'form.legend'|trans }}</legend>
        {{ form_widget(form) }}
        <div class="control-group">
            <div class="controls">
                <button class="btn btn-success" type="submit"><i class="symbol">plus</i> {{ 'button.create'|trans }}</button>
                <a class="btn" href="{{ path('tracker_index') }}">{{ 'button.cancel'|trans }}</a>
            </div>
        </div>
        </form>
    </div>
{% endblock %}
